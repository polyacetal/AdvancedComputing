#!/usr/bin/awk -f

BEGIN{
	FS=",";
}
NR == 1{print $1, $2, "0~4歳", "20~24歳"}
NR == 2{
	all=$2
	all04=$3+$4+$5+$6+$7;
	all2024=$23+$24+$25+$26+$27;
	print $1, $2"人", $3+$4+$5+$6+$7"人", $23+$24+$25+$26+$27"人"}
NR >= 3{print $1, $2/all*100"%", ($3+$4+$5+$6+$7)/all04*100"%", ($23+$24+$25+$26+$27)/all2024*100"%"}
